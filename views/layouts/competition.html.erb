<!DOCTYPE html>
<html lang="en">
  <%= render "shared/layout_head" %>
  <body id = <%= body_id %> >
    <%= render "shared/header" %>
    <div id = "container">
      <%= render "shared/banner", image: @competition.banner.url(:banner) %>
      <%= render "competitions/comp_banner", competition: @competition %>
      <%= render "competitions/nav2"%>
      <%= yield %>
    </div>
    <%= render "shared/footer" %>
  </body>
</html>
